# Jellyfin Media Downloader Environment Configuration
# Copy this file to .env and update with your values

# Jellyfin server URL (required)
JELLYFIN_URL=https://your-jellyfin-server.com

# Jellyfin credentials (required)
USER_NAME=your_username
JELLYFIN_PASSWORD=your_password

# Download directory inside container
DOWNLOAD_PATH=/data

# Dry run mode - set to true to test without downloading
DRY_RUN=false
